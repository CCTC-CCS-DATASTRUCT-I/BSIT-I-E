#include <iosstream>
using namespace std;
 
 int main (){
    int Coffee;
    cout << "Espresso,Americano<<cappuccino,Latte";
    cin >> Coffee;

    switch(coffee){
        case 1:
        cout << "Espresso"
        break;
        case 2:
        cout<< "Americano";
        break;
        case 3;
        cout<<"Cappuccino"
        break;
        case 4;
        cout<< "Latte"
        break; 
    }

 }
          

